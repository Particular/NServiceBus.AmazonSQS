<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net472;net6.0</TargetFrameworks>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NServiceBus.snk</AssemblyOriginatorKeyFile>
    <LangVersion>10.0</LangVersion>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>NServiceBus.AmazonSQS</PackageId>
    <Description>An Amazon SQS transport implementation for NServiceBus</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.S3" Version="[3.7.7.25, 3.8.0)" />
    <PackageReference Include="AWSSDK.SimpleNotificationService" Version="[3.7.3.32, 3.8.0)" />
    <PackageReference Include="AWSSDK.SQS" Version="[3.7.2.29, 3.8.0)" />
    <PackageReference Include="NServiceBus" Version="[8.0.0, 9)" />
    <PackageReference Include="Fody" Version="6.6.0" PrivateAssets="All" />
    <PackageReference Include="Obsolete.Fody" Version="5.3.0" PrivateAssets="All" />
    <PackageReference Include="Particular.Packaging" Version="2.1.0" PrivateAssets="All" />
  </ItemGroup>

</Project>
